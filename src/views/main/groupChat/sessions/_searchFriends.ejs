<% users.forEach( (user) => { %>
<div data-uid="<%= user._id %>">
  <li data-uid="<%= user._id %>">
      <div class="contactPanel">
          <div class="user-avatar">
              <img <% if(user.avatar == "avatar-default.jpg") {%> src="/images/users/default/<%= user.avatar %>" <%} else {%>  src="/images/users/<%= user.avatar %>" <%}%> alt="">
          </div>
          <div class="user-name">
              <p>
                  <%= user.username %>
              </p>
          </div>
          <br>
          <div class="user-address">
              <span>&nbsp;<%= user.address ? user.address : "Chưa cập nhật" %></span>
          </div>
          <div class="add-user" data-uid="<%= user._id %>">
              Thêm vào nhóm
          </div>
      </div>
  </li>
</div>

<%})%>